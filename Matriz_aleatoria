#include<iostream>
#include<stdlib.h> //libreria estandar

int main(){
  int matrix[3][3];
  int fil;
  int col;
  int num;
  int salir;
  
  for(fil=0; fil<3; fil++){
    for(col=0; col<3; col++){
      num=rand()%9;
      std::cout<<num<<" ";
      matrix[fil][col]= num;
    
    }
   
   std::cout<<"\n";
   
  }
  
  std::cin>>salir;
  return 0;

}
